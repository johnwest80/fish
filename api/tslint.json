{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "max-line-length": [
      true,
      140
    ],
    "no-console": {
      "severity": "warning"
    },
    "trailing-comma": [
      false
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "rulesDirectory": []
}