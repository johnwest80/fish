{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      false,
      "single"
    ],
    "max-line-length": [
      true,
      140
    ],
    "no-console": {
      "severity": "warning"
    },
    "trailing-comma": [
      false
    ],
    "object-literal-key-quotes": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "space-before-function-paren": false,
    "arrow-parens": false
  },
  "rulesDirectory": []
}